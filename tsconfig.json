{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"lib": ["ESNext", "DOM.Iterable"],
		"moduleResolution": "bundler",
		"jsx": "preserve",
		"resolveJsonModule": true,
		"strict": true,
		"skipLibCheck": true,
		"useUnknownInCatchVariables": false,
		"allowImportingTsExtensions": true,
		"allowSyntheticDefaultImports": true
	},
}
